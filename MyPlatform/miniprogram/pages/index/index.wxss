page{
  --gradualBlue: linear-gradient(45deg, #0081ff, #1cbbb4);
  --ShadowSize: 6rpx 6rpx 8rpx;
  --white: #ffffff;
  height:100%;
  background-color: #dedede;
}

.main{
  height:100%;

}

.top-bar{
  background-image: var(--gradualBlue);
  height:250rpx;
  width:100%;
  border-bottom-left-radius: 30rpx;
  border-bottom-right-radius: 30rpx;
  box-shadow:10rpx 5rpx 5rpx #15acc8;

  display: flex;
}
.top-bar-image{
  margin-top:40rpx;
  margin-left:75rpx;
  width: 200rpx;
	height: 200rpx;
	background-size: cover;
	background-position: center;
  z-index:99;
  background-image: url('data:image/png;base64,')
}
.top-bar-title{
  margin-top:75rpx;
  width: 320rpx;
	height: 120rpx;
	background-size: cover;
	background-position: center;
  z-index:99;
  background-image: url('data:image/png;base64,');
}

.middle-bar{
  width:85%;
  margin:40rpx auto;
  height:400rpx;
  background-color: #FFFFFF;
  box-shadow:10rpx 5rpx 5rpx #c5c9ca;
  border-radius: 20rpx;
  border-style:solid;
  border-color:#c5c9ca;
  border-width:0.2rpx;
  display:flex;
  flex-direction:row;
  flex-wrap:wrap;
  padding-left:30rpx;
}

.middle-bar-item{
  width:150rpx;
  height:145rpx;
  margin-left:50rpx;
  margin-top:20rpx;
}

.middle-bar-image{
  width:90%;
  height:90%;
}

.middle-bar-text{
  margin-left:15rpx;
  font-size:small;
}

.scroll-bar{
width:100%;
height:100%;
margin-top:-40rpx;
}

.scroll-item{
  width:90%;
  height:220rpx;
  margin:30rpx auto;
  background-color: #ffffff;
  box-shadow:10rpx 5rpx 5rpx #c5c9ca;
  border-radius: 20rpx;
  display:flex;
}

.scroll-item-image{
  margin-top:30rpx;
  margin-left:20rpx;
  width:220rpx;
  height:150rpx;
  border-radius: 20rpx;
}

.scroll-item-textfield{
  margin-left:50rpx;
  margin-top:20rpx;
  width:350rpx;
}

.scroll-item-title{
  font-size: 35rpx;
}

.scroll-item-content{
  height:100rpx;
  font-size:25rpx;
  overflow: hidden;
}

.scroll-item-gap{
  width:300rpx;
  height:5rpx;
  border-radius: 100rpx;
  margin-top:10rpx;
  margin-bottom: 10rpx;
  background-color: #c5c9ca;
}